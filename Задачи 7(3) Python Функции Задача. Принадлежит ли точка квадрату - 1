def is_in(x, y):
    return ((abs(x) <= 1) and (abs(y) <= 1))
print(is_in(int(input()), int(input())))


def ipis(x,y):
      if abs(x)<=1 and abs(y)<=1:
            return True
      else:
            return False
 
 
def IsPointInSquare(x,y):
      return ipis(x,y)
      
      
import math
def circle(x, y):
    if abs(x) <=1 and abs(y) <=1:
        print("YES")
    else:
        print("NO")
x = float(input())
y = float(input())
circle(x, y)


def ipis(x,y):
      if abs(x)<=1 and abs(y)<=1:
            return True
      else:
            return False
 
def IsPointInSquare(x,y):
      return ipis(x,y)
