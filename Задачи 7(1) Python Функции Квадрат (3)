from math import sqrt
def square(x):
    return([float(x * 4), float(x ** 2), float(sqrt(2) * x)])
print(*square(float(input())))


import math
def square(a):
    a = (4*a,a**2,math.sqrt(2)*a,)
    return a
    
    
def square(a=int(input())):
    print(a*4)
    print(a**2)
    print(a**2+a**2)

square()    
